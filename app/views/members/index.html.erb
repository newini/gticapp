<div class="row">

  <div class="col-md-2">
    <%= render'members/side' %>
  </div>

  <div class="col">
    <%= link_to 'ダウンロード', members_path(format: 'xls'), class: "btn btn-light btn-sm" %>
    <% if admin? %>
      <%= link_to "Change name to alphabet", :name_to_alphabet_members, class: "btn btn-info btn-sm", method: "POST" %>
      <%= link_to "Convert FB ID from string", :convert_fb_str_to_id_members, class: "btn btn-info btn-sm", method: "POST" %>
    <% end %>

    <div id="ajax_container">
      <%= render'members' %>
    </div>
  </div>

</div>
