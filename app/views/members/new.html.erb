<div class="row-fluid">
  <aside class="span2">
    <section>
      <%= render'members/side' %>
    </section>
  </aside>
  <div class="span5">
    <%= render 'member_form' %>
  </div>
  <div class="span5">
    <h2>メンバー情報CSVデータ取り込み</h2>
      <%= form_tag import_members_path, multipart: true do %>
        <%= file_field_tag :file, class: "file-holder" %>
      <%= submit_tag "CSV読込",class: "btn" %>
    <% end %>
  </div>
</div>
