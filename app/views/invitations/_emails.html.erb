<%= content_tag :table, class: 'table table-condensed table-bordered table-striped' do %>
  <tr>
    <th class="col-md-4">Email</th>
    <th></th>
  </tr>
  <% @emails.each do |email| %>
    <%= content_tag :tr do %>
      <td><%= email %></td>
      <td><%= select_to_delete(@invitation, ) %></td>
    <% end %>
  <% end %>
<% end %>
