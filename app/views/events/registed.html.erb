<div class="row">
  <aside class="col-sm-2">
    <%= render'side' %>
  </aside>
  <div class="col-sm-10">
    <div class="btn-group">
      <%= link_to registed_event_path, class: "btn btn-default" do %>
        <span class="glyphicon glyphicon-th-list"></span> 参加予定者一覧
      <% end %>
      <%= link_to update_registed_member_event_path, class: "btn btn-default", remote: true do %>
        <span class="glyphicon glyphicon-edit"></span> 参加予定者情報編集
      <% end %>
      <%= link_to waiting_event_path, class: "btn btn-default", remote: true do %>
        <span class="glyphicon glyphicon-plus"></span> 参加予定者追加
      <% end %>
      <%= link_to update_facebook_event_path, method: "post", class:"btn btn-default" do %>
          <span class="glyphicon glyphicon-refresh"></span> <s>facebook情報更新</s>(Not work!)
      <% end %>
      <%= link_to registed_list_event_path, method: "get", class:"btn btn-default" do %>
        <span class="glyphicon glyphicon-th-list"></span> 参加者リスト
      <% end %>
      <%= link_to registed_event_path(format: 'xls'), method: "get", class: "btn btn-default" do %>
        <span class="glyphicon glyphicon-download"></span> Download
      <% end %>

    </div>
    <div id="ajax_container">
      <%= render'members' %>
    </div>
  </div>
</div>
