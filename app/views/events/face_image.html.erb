<h2><%= @title %>　顔写真</h2>
  <%= content_tag :table, class: 'table table-condensed table-bordered table-striped' do %>
<!--    <tr>
      <th class="col-md-1">写真</th>
    </tr>-->
    <% i = 1 %>
    <% @members.each do |member| %>
      <%= content_tag :tr do %>
        <% if !member.gtic_flg && member.fb_user_id.present? %>
          <%= image_tag "https://graph.facebook.com/#{member.fb_user_id}/picture?type=normal" %>
          <%= "#{member.fb_name}" %>
        <% end %>
      <% end %>
      <% i += 1 %>
    <% end %>
  <% end %>
</div>

<div id="ajax-form">
</div>

